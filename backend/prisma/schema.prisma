
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int     @id @default(autoincrement())
  code  Int     @unique @default(autoincrement())
  name  String  @unique
  password  String
  admin Boolean @default(false)
}
